{
  "name": "react-webpack-starter",
  "version": "2.0.0",
  "description": "Basic packages and scripts to start app developing in react, redux and webpack",
  "main": "webpack.config.js",
  "scripts": {
    "start": "npm run server:dev",
    "server:dev": "webpack-dev-server --config webpack/webpack-dev.config.js",
    "server:prod": "webpack-dev-server --config webpack/webpack-prod.config.js",
    "build": "webpack --progress --colors --config webpack/webpack-prod.config.js",
    "build:prod": "npm run build",
    "build:dev": "webpack --progress --colors --config webpack/webpack-dev.config.js",
    "npm-check": "npm-check --skip-unused",
    "npm-check:update": "npm-check --update",
    "test": "BABEL_DISABLE_CACHE=1 jest --verbose",
    "eslint": "./node_modules/eslint/bin/eslint.js ./source/**/*.js ./source/**/**/*.js ./source/**/*.jsx ./source/**/**/*.jsx",
    "watch": "npm run watch:dev",
    "watch:dev": "webpack --progress --colors --watch --config webpack/webpack-dev.config.js",
    "analyzer": "webpack --config webpack/webpack-prod.config.js --json | webpack-bundle-size-analyzer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artemdemo/react-webpack-starter.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/artemdemo/react-webpack-starter/issues"
  },
  "homepage": "https://github.com/artemdemo/react-webpack-starter#readme",
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-dom"
    ]
  },
  "dependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.27.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "history": "^4.6.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "lodash": "^4.17.4",
    "material-ui": "^0.17.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-form": "^6.6.0",
    "redux-form-material-ui": "^4.1.2",
    "redux-logger": "^2.8.2",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "axios": "^0.15.3",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "clean-webpack-plugin": "^0.1.15",
    "enzyme": "^2.7.1",
    "eslint": "^3.17.1",
    "eslint-config-artem-react": "git+https://github.com/artemdemo/eslint-config-artem-react.git#2.1.0",
    "jest-cli": "^19.0.2",
    "keymirror": "^0.1.1",
    "lodash-webpack-plugin": "^0.11.2",
    "node-sass": "^4.5.0",
    "npm-check": "^5.4.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "redux-mock-store": "^1.2.2",
    "sass-loader": "^6.0.3",
    "webpack-dev-server": "^2.4.1"
  }
}
